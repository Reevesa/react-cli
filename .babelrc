/**
* http://babeljs.io/docs/plugins/preset-env/
*/
{
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }]
    ]
}
